{
  "name": "externality",
  "version": "0.2.2",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext .ts .",
    "prepack": "pnpm build",
    "release": "pnpm test && standard-version && git push --follow-tags && pnpm publish",
    "test": "pnpm lint && vitest run"
  },
  "dependencies": {
    "enhanced-resolve": "^5.10.0",
    "mlly": "^0.5.14",
    "pathe": "^0.3.7",
    "ufo": "^0.8.5"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@types/node": "^17.0.41",
    "@vitest/coverage-c8": "^0.23.4",
    "allowlist": "^0.1.1",
    "c8": "^7.12.0",
    "eslint": "^8.23.1",
    "rollup": "^2.79.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^2.9.15",
    "vitest": "^0.23.4",
    "webpack": "^5.74.0"
  },
  "build": {
    "externals": [
      "rollup"
    ]
  },
  "packageManager": "pnpm@7.11.0"
}
