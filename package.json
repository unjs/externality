{
  "name": "externality",
  "version": "0.2.2",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext .ts .",
    "prepack": "pnpm build",
    "release": "pnpm test && standard-version && git push --follow-tags && pnpm publish",
    "test": "pnpm lint && vitest run"
  },
  "dependencies": {
    "enhanced-resolve": "^5.9.3",
    "mlly": "^0.5.2",
    "pathe": "^0.3.0",
    "ufo": "^0.8.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^10.0.0",
    "@types/node": "^17.0.41",
    "allowlist": "^0.1.1",
    "c8": "^7.11.3",
    "eslint": "^8.17.0",
    "rollup": "^2.75.6",
    "standard-version": "^9.3.2",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.14.2",
    "webpack": "^5.73.0"
  },
  "build": {
    "externals": [
      "rollup"
    ]
  },
  "packageManager": "pnpm@7.2.0"
}
