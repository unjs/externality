{
  "name": "externality",
  "version": "0.1.6",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext .ts .",
    "prepack": "yarn build",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && vitest run"
  },
  "dependencies": {
    "allowlist": "^0.1.1",
    "enhanced-resolve": "^5.8.3",
    "mlly": "^0.3.10",
    "pathe": "^0.2.0",
    "ufo": "^0.7.9"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@types/node": "^17.0.9",
    "eslint": "^8.7.0",
    "rollup": "^2.58.0",
    "standard-version": "^9.3.1",
    "unbuild": "^0.7.0",
    "vite": "^2.8.6",
    "vitest": "^0.6.0",
    "webpack": "^5.58.2"
  },
  "build":{
    "externals": ["rollup"]
  }
}
