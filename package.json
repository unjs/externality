{
  "name": "externality",
  "version": "1.0.0",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext .ts .",
    "prepack": "pnpm build",
    "release": "pnpm test && standard-version && git push --follow-tags && pnpm publish",
    "test": "pnpm lint && vitest run"
  },
  "dependencies": {
    "enhanced-resolve": "^5.12.0",
    "mlly": "^1.0.0",
    "pathe": "^1.0.0",
    "ufo": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@vitest/coverage-c8": "^0.25.3",
    "allowlist": "^0.1.1",
    "c8": "^7.12.0",
    "eslint": "^8.29.0",
    "eslint-config-unjs": "^0.0.2",
    "rollup": "^3.5.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.9.3",
    "unbuild": "^1.0.1",
    "vite": "^3.2.4",
    "vitest": "^0.25.3",
    "webpack": "^5.75.0"
  },
  "build": {
    "externals": [
      "rollup"
    ]
  },
  "packageManager": "pnpm@7.17.1"
}
