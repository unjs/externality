{
  "name": "externality",
  "version": "0.2.1",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest",
    "lint": "eslint --ext .ts .",
    "prepack": "yarn build",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && vitest run"
  },
  "dependencies": {
    "enhanced-resolve": "^5.9.2",
    "mlly": "^0.5.1",
    "pathe": "^0.2.0",
    "ufo": "^0.8.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^9.0.0",
    "@types/node": "^17.0.23",
    "allowlist": "^0.1.1",
    "c8": "^7.11.0",
    "eslint": "^8.12.0",
    "rollup": "^2.70.1",
    "standard-version": "^9.3.1",
    "unbuild": "^0.7.2",
    "vite": "^2.9.1",
    "vitest": "^0.8.4",
    "webpack": "^5.71.0"
  },
  "build": {
    "externals": [
      "rollup"
    ]
  }
}
